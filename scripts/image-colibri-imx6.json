{
  "config_format": 1,
  "isinstaller": true,
  "name": "Toradex Easy Installer",
  "description": "Toradex Easy Installer for Colibri iMX6.",
  "icon": "tezi.png",
  "version": "V0.6",
  "release_date": "2017-01-30",
  "wrapup_script": "wrapup.sh",
  "autoinstall": false,
  "supported_product_ids": [
    "0014",
    "0015",
    "0016",
    "0017"
  ],
  "blockdevs": [
    {
      "name": "mmcblk0",
      "partitions": [
        {
          "partition_size_nominal": 64,
          "want_maximised": false,
          "content": {
            "label": "BOOT",
            "filesystem_type": "FAT",
            "mkfs_options": "",
            "filelist": [ "boot.scr", "tezi.itb" ],
            "uncompressed_size": 21
          }
        }
      ]
    },
    {
      "name": "mmcblk0boot0",
      "content": {
        "filesystem_type": "raw",
        "rawfiles": [
          {
            "filename": "SPL",
            "dd_options": "seek=2"
          },
          {
            "filename": "u-boot.img",
            "dd_options": "seek=138"
          }
        ]
      }
    }
  ]
}
